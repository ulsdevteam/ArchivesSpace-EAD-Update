  <dsc><c01 id="aspace_ref13_rly" level="series"><did><unittitle>Founding and Organizational Documents</unittitle><unitid>I.</unitid><unitdate datechar="creation" normal="1975/1989" type="inclusive">1975-1989</unitdate></did><scopecontent id="aspace_288e5a950f71e33081e3055288881019"><head>Scope and Contents note</head><p>This series contains information regarding the founding of the Denominational Ministry Strategy (DMS) and its predecessors, the Pittsburgh Coalition and Metropolitan Citizens Organization. Included are mission statements, meeting minutes, yearbooks, and correspondence from the organizations' founders. Papers on Mellon Bank, U.S. Steel, and the Network to Save the Mon/Ohio Valley are also present and provide an overview of their relationship with the DMS.</p></scopecontent><c02 id="aspace_ref14_jao" level="file"><did><unittitle>Pittsburgh Coalition</unittitle><unitdate datechar="creation" normal="1976/1980" type="inclusive">1976-1980</unitdate><container id="aspace_2ac3e24cc5ad7ec5251c5b61a7c95d65" label="Mixed Materials" type="box">1</container><container id="aspace_4d02f109e02bf4dda5ac96e208a2e95d" parent="aspace_2ac3e24cc5ad7ec5251c5b61a7c95d65" type="folder">1</container></did></c02><c02 id="aspace_ref15_g95" level="file"><did><unittitle>Pittsburgh Coalition Founding and Organizational Documents</unittitle><unitdate datechar="creation" normal="1976/1980" type="inclusive">1976-1980</unitdate><container id="aspace_4b39ba1af66b52fc8d358af3cf773eb8" label="Mixed Materials" type="box">1</container><container id="aspace_362c7396958ffa928c2260a9c7fc236a" parent="aspace_4b39ba1af66b52fc8d358af3cf773eb8" type="folder">2</container></did></c02><c02 id="aspace_ref16_s5h" level="file"><did><unittitle>Pittsburgh Coalition Correspondence</unittitle><unitdate datechar="creation" normal="1976/1980" type="inclusive">1976-1980</unitdate><container id="aspace_5c6be3338834b95cda87383719100f06" label="Mixed Materials" type="box">1</container><container id="aspace_7ba2684e3ce8c931fd8a351d364aaac3" parent="aspace_5c6be3338834b95cda87383719100f06" type="folder">3</container></did></c02><c02 id="aspace_ref17_7fy" level="file"><did><unittitle>Pittsburgh Coalition Meeting Minutes</unittitle><unitdate datechar="creation" normal="1975/1979" type="inclusive">1975-1979</unitdate><container id="aspace_f211cb3f23ed67b8b55a2a6f85e7f5de" label="Mixed Materials" type="box">1</container><container id="aspace_a88915f1a9cd3709e860a116f718f4d0" parent="aspace_f211cb3f23ed67b8b55a2a6f85e7f5de" type="folder">4</container></did></c02><c02 id="aspace_ref18_k31" level="file"><did><unittitle>Pittsburgh Coalition Newsletters and Mailings</unittitle><unitdate datechar="creation" normal="1976/1979" type="inclusive">1976-1979</unitdate><container id="aspace_111784f73f9497d6883163a36b627611" label="Mixed Materials" type="box">1</container><container id="aspace_8ff47fe8b2ac76d55792a16bc2c905d8" parent="aspace_111784f73f9497d6883163a36b627611" type="folder">5</container></did></c02><c02 id="aspace_ref19_3f8" level="file"><did><unittitle>Metropolitan Citizens Organization Yearbook</unittitle><unitdate datechar="creation" normal="1978/1982" type="inclusive">1978-1982</unitdate><container id="aspace_bd547147b6155b6136a93410dfbf50a6" label="Mixed Materials" type="box">1</container><container id="aspace_0578062ef836fed6fb490cd1cec7f62e" parent="aspace_bd547147b6155b6136a93410dfbf50a6" type="folder">6</container></did></c02><c02 id="aspace_ref20_0xu" level="file"><did><unittitle>Denominational Ministry Strategy Founding and Organizational Documents</unittitle><unitdate datechar="creation" normal="1982/1984" type="inclusive">1982-1984</unitdate><container id="aspace_34d9f4f7f5bfadb8e78a8dd3cd89076a" label="Mixed Materials" type="box">1</container><container id="aspace_6461102553c87e07c45f6ccd2467050c" parent="aspace_34d9f4f7f5bfadb8e78a8dd3cd89076a" type="folder">7</container></did></c02><c02 id="aspace_ref21_17u" level="file"><did><unittitle>Denominational Ministry Strategy Ideological and Theological Documents</unittitle><unitdate datechar="creation" normal="1983/1986" type="inclusive">1983-1986</unitdate><container id="aspace_8398c45515408b2e71761ee2cf520436" label="Mixed Materials" type="box">1</container><container id="aspace_4a0e0c47f862e9df6af0a52fac120361" parent="aspace_8398c45515408b2e71761ee2cf520436" type="folder">8</container></did></c02><c02 id="aspace_ref22_50l" level="file"><did><unittitle>Denominational Ministry Strategy</unittitle><unitdate datechar="creation" normal="1983/1988" type="inclusive">1983-1988</unitdate><container id="aspace_ab1f04e70c445ab8df2b82cccb37c832" label="Mixed Materials" type="box">1</container><container id="aspace_9399fde83a6067a9674f62f6e79f17d4" parent="aspace_ab1f04e70c445ab8df2b82cccb37c832" type="folder">09-11</container></did></c02><c02 id="aspace_ref23_zlx" level="file"><did><unittitle>Mellon Bank</unittitle><unitdate datechar="creation" normal="1977/1984" type="inclusive">1977-1984</unitdate><container id="aspace_4b642ee2242361c5a169de11a643febf" label="Mixed Materials" type="box">1</container><container id="aspace_42b51a5974ccc8de25b0515b0ec0cde7" parent="aspace_4b642ee2242361c5a169de11a643febf" type="folder">12</container></did></c02><c02 id="aspace_ref24_rta" level="file"><did><unittitle>U.S. Steel/USX</unittitle><unitdate datechar="creation" normal="1979/1983" type="inclusive">1979-1983</unitdate><container id="aspace_efd34722efc3a426a0bdc0679a18421e" label="Mixed Materials" type="box">1</container><container id="aspace_9ba53cd2b040f7e843c5a44baaf6aa00" parent="aspace_efd34722efc3a426a0bdc0679a18421e" type="folder">13</container></did></c02><c02 id="aspace_ref25_0b8" level="file"><did><unittitle>Network to Save the Mon/Ohio Valley</unittitle><unitdate datechar="creation" normal="1983/1984" type="inclusive">1983-1984</unitdate><container id="aspace_e12fa4d1fe40abc3ef8c8e4532e8ebd5" label="Mixed Materials" type="box">1</container><container id="aspace_d794868f0b4375d9828a7c1833f3e973" parent="aspace_e12fa4d1fe40abc3ef8c8e4532e8ebd5" type="folder">14</container></did></c02><c02 id="aspace_ref26_wb6" level="file"><did><unittitle>Confessing Synod Ministries</unittitle><unitdate datechar="creation" normal="1987/1989" type="inclusive">1987-1989</unitdate><container id="aspace_7a7f6a28703899e0dbde0f1f289d6322" label="Mixed Materials" type="box">1</container><container id="aspace_4b74808c496cba3ef12ac65db4eb68e3" parent="aspace_7a7f6a28703899e0dbde0f1f289d6322" type="folder">15</container></did></c02><c02 id="aspace_ref27_lt7" level="file"><did><unittitle>Denominational Ministry Strategy Extended</unittitle><unitdate datechar="creation" normal="1987/1989" type="inclusive">1987-1989</unitdate><container id="aspace_e75a33fe2915d55ed352dd5750ce03df" label="Mixed Materials" type="box">1</container><container id="aspace_a7315830c891370bb8df04ec693331b6" parent="aspace_e75a33fe2915d55ed352dd5750ce03df" type="folder">16</container></did></c02></c01><c01 id="aspace_ref29_9pn" level="series"><did><unittitle>Newsletters</unittitle><unitid>II.</unitid><unitdate datechar="creation" normal="1986/1995" type="inclusive">1986-1995</unitdate></did><scopecontent id="aspace_bc803eef33526d416a0595f643d21603"><head>Scope and Contents note</head><p>The newsletters in this series document the activities of the DMS, DMX, and Confessing Synod Ministries. Many of the activities are concerned with the deindustrialization of Pittsburgh in the 1980s and include information on actions taken against Mellon Bank, Dravo Corporation, and U.S. Steel. Newsletters from the Christ Lutheran Church and St. Matthew's Episcopal Church, both members of the DMS, are also included.</p></scopecontent><c02 id="aspace_ref30_6wx" level="file"><did><unittitle>Christ Lutheran Church</unittitle><unitdate datechar="creation" normal="1986/1989" type="inclusive">1986-1989</unitdate><container id="aspace_1ada465b20e5a3b199f721026f2ae6b2" label="Mixed Materials" type="box">1</container><container id="aspace_9de45fe67d3c43517031adf2ce05ca8c" parent="aspace_1ada465b20e5a3b199f721026f2ae6b2" type="folder">17</container></did></c02><c02 id="aspace_ref31_7ta" level="file"><did><unittitle><title xlink:type="simple">The Voice of the Valley</title>, St. Matthew's Episcopal Church</unittitle><unitdate datechar="creation" normal="1987/1988" type="inclusive">1987-1988</unitdate><container id="aspace_f79f35ae2f38b4a8c5be57f390a954b2" label="Mixed Materials" type="box">1</container><container id="aspace_df564ed395315f587cf965fe459182f8" parent="aspace_f79f35ae2f38b4a8c5be57f390a954b2" type="folder">18</container></did></c02><c02 id="aspace_ref32_hlo" level="file"><did><unittitle>Denominational Ministry Strategy</unittitle><unitdate datechar="creation" normal="1987/1987" type="inclusive">1987</unitdate><container id="aspace_2fb3e5a493c7925fb5237e0bdbf06a44" label="Mixed Materials" type="box">1</container><container id="aspace_dee3dc24ea38e615c01f64b0c968fd12" parent="aspace_2fb3e5a493c7925fb5237e0bdbf06a44" type="folder">19</container></did></c02><c02 id="aspace_ref33_l9j" level="file"><did><unittitle>Denominational Ministry Strategy</unittitle><unitdate datechar="creation">January - April 1988</unitdate><container id="aspace_c612b82153be1d429509db14565ce1b2" label="Mixed Materials" type="box">1</container><container id="aspace_72290e76f5e53d9f1df2b04d8c2d02d5" parent="aspace_c612b82153be1d429509db14565ce1b2" type="folder">20</container></did></c02><c02 id="aspace_ref34_3pm" level="file"><did><unittitle>Confessing Synod Ministries</unittitle><unitdate datechar="creation" normal="1988/1995" type="inclusive">1988-1995</unitdate><container id="aspace_cf90907357737c43ee5b7df52bd0cafd" label="Mixed Materials" type="box">1</container><container id="aspace_8ef96f2cd3b62b0151a96033fbeea745" parent="aspace_cf90907357737c43ee5b7df52bd0cafd" type="folder">21-25</container></did></c02></c01><c01 id="aspace_ref36_ha1" level="series"><did><unittitle>Press and Media Releases</unittitle><unitid>III.</unitid><unitdate datechar="creation" normal="1982/1987" type="inclusive">1982-1987</unitdate></did><scopecontent id="aspace_25cb0c3e6d511f15dc452113b3993e5d"><head>Scope and Contents note</head><p>This series contains press releases regarding the activities of the DMS and the companies the DMS held responsible for Pittsburgh's high unemployment in the 1980s. Many of the press releases are concerned with protests mounted jointly by the DMS and the Network to Save the Mon/Ohio Valley and the actions of corporate executives from Mellon Bank and U.S. Steel. Also included are examples of pamphlets which would be distributed throughout Pittsburgh communities concerning the high unemployment rates and economic fall of the region.</p></scopecontent><c02 id="aspace_ref37_8fh" level="file"><did><unittitle>Press Releases and Announcements</unittitle><unitdate datechar="creation" normal="1982/1983" type="inclusive">1982-1983</unitdate><container id="aspace_6b634aa26d4db698c40441feeb67cefa" label="Mixed Materials" type="box">2</container><container id="aspace_074404c7ae6c4c0c268d5f655c1f11e0" parent="aspace_6b634aa26d4db698c40441feeb67cefa" type="folder">1</container></did></c02><c02 id="aspace_ref38_oos" level="file"><did><unittitle>Press Releases</unittitle><unitdate datechar="creation">January 1984 - January 1987</unitdate><container id="aspace_f8f1f8b2412e4b3a7bfe134792e30fd1" label="Mixed Materials" type="box">2</container><container id="aspace_853bb46b84d33011b1b043e241c8c9dd" parent="aspace_f8f1f8b2412e4b3a7bfe134792e30fd1" type="folder">02-08</container></did></c02><c02 id="aspace_ref39_4a2" level="file"><did><unittitle>Community Releases,1985-1986</unittitle><unitdate datechar="creation">undated</unitdate><container id="aspace_d00648fb72e1e679a7b6936c94deae22" label="Mixed Materials" type="box">2</container><container id="aspace_977838c6d37384634ca2a22ca7f87535" parent="aspace_d00648fb72e1e679a7b6936c94deae22" type="folder">09-10</container></did></c02></c01><c01 id="aspace_ref41_9m0" level="series"><did><unittitle>Legal Documents</unittitle><unitid>IV.</unitid><unitdate datechar="creation" normal="1984/1994" type="inclusive">1984-1994</unitdate></did><scopecontent id="aspace_5b00977b146d77facdb899181e1d3bbb"><head>Scope and Contents note</head><p>The documents in this series are concerned mainly with the legal troubles of Rev. D. Douglas Roth. Early in 1984, Roth was removed from his position at the Trinity Lutheran Church in Clairton, Pa., by the bishop of the Western Pennsylvania-West Virginia Synod of the Lutheran Church in America. Roth ignored the order so that he would be granted a disciplinary hearing before church authorities to plead his case; however, the bishop filed a civil court complaint instead. Roth was jailed in contempt of court for failing to appear at his hearing and for conducting services. He remained in jail for several months because he refused to sign a paper that would end his term at Trinity Lutheran Church. Also included are legal documents regarding the DMX's resistance to developers George and Damien Zamias erecting a shopping mall in Frazer Township, Pa.</p></scopecontent><c02 id="aspace_ref42_1q1" level="file"><did><unittitle><emph render="italic">Bishop Kenneth May and Western PA - West Virginia Synod v. Trinity Lutheran Church and D. Douglas Roth </emph></unittitle><unitdate datechar="creation" normal="1982/1983" type="inclusive">1982-1983</unitdate><container id="aspace_0687d42b08f9bec0e30ec2af2422ffbf" label="Mixed Materials" type="box">2</container><container id="aspace_e48f540041c438a0d4bd0d27be30de2f" parent="aspace_0687d42b08f9bec0e30ec2af2422ffbf" type="folder">11-15</container></did></c02><c02 id="aspace_ref43_y25" level="file"><did><unittitle><emph render="italic">Commonwealth v. D. Douglas Roth v. Rev. William Rex Jr.</emph> Transcript</unittitle><unitdate datechar="creation">undated</unitdate><container id="aspace_efc78a6fa19de414b9471ab2d2aa499c" label="Mixed Materials" type="box">2</container><container id="aspace_5e10727433e0b8b8d7760af9fa596548" parent="aspace_efc78a6fa19de414b9471ab2d2aa499c" type="folder">16</container></did></c02><c02 id="aspace_ref44_jcr" level="file"><did><unittitle><emph render="italic"> Bishop Kenneth May and Western PA-West Virginia Synod v. Trinity Lutheran Church, D. Douglas Roth and Nadine Roth </emph> Transcript</unittitle><unitdate datechar="creation" normal="1985/1985" type="inclusive">1985</unitdate><container id="aspace_2ee76f415bb2cad7c36a883a4a0015cb" label="Mixed Materials" type="box">2</container><container id="aspace_f673f97a3a81ab0d231c7c14f6384d6b" parent="aspace_2ee76f415bb2cad7c36a883a4a0015cb" type="folder">17</container></did></c02><c02 id="aspace_ref45_ml0" level="file"><did><unittitle><emph render="italic"> Bishop Kenneth May and Western PA-West Virginia Synod v. Trinity Lutheran Church, D. Douglas Roth and Nadine Roth</emph> Transcript</unittitle><unitdate datechar="creation" normal="1988/1988" type="inclusive">1988</unitdate><container id="aspace_695e92ffd08004ca88c1b2eab244bfa6" label="Mixed Materials" type="box">2</container><container id="aspace_2d70ab19bae513e1fc82242698f1c2a9" parent="aspace_695e92ffd08004ca88c1b2eab244bfa6" type="folder">18</container></did></c02><c02 id="aspace_ref46_o0r" level="file"><did><unittitle><emph render="italic">Commonwealth v. Dewey Douglas Roth</emph> Transcript</unittitle><unitdate datechar="creation" normal="1985/1985" type="inclusive">1985</unitdate><container id="aspace_14625263d1593476748a322402a0f588" label="Mixed Materials" type="box">2</container><container id="aspace_fe74a8fb7b0b39608bc10e0e670ca878" parent="aspace_14625263d1593476748a322402a0f588" type="folder">19-20</container></did></c02><c02 id="aspace_ref47_08w" level="file"><did><unittitle><emph render="italic">Bishop Kenneth May and Western PA-West Virginia Synod v. Trinity Lutheran Church, D. Douglas Roth and Nadine Roth</emph> Legal Documents</unittitle><unitdate datechar="creati